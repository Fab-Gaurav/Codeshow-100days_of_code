#include <iostream>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int a,b;
        cin>>a>>b;
        int x=0,y=0;
        for (int i=1;a>=0;i=i+2){
            a=a-i;
            x++;
        }
    for(int i=2;b>=0;i=i+2){
        b=b-i;
        y++;
    }
        if(x<y)
        cout<<"Bob"<<endl;
        else if(x>y)
        cout<<"Limak"<<endl;
        else
        cout<<"Bob"<<endl;
        
    }
    

    return 0;
}
