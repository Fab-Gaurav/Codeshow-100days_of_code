#include <iostream>
using namespace std;

int main()
{
    int t,p1,p2,k,sum=0;
    cin>>t;
    while(t--){
        cin>>p1>>p2>>k;
        sum=p1+p2;
        if((sum/k)%2==0){
            cout<<"CHEF"<<endl;
        }
            else
            cout<<"COOK"<<endl;
            
    }
        
    return 0;
}
