
#include <iostream>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int a[30],i;
        for(i=0;i<30;i++)
        cin>>a[i];
        int count=0,flag=0;
        for(i=0;i<30;i++)
        {
            if(a[i]==1)
            {
                count ++;
            }
            else 
            count=0;
            if(count>5)
            {
                flag=1;
                break;
            }
            if(flag==0)
            cout<<"#allcodersarefun"<<endl;
            else
            cout<<"#coderlifematters"<<endl;
        }
    }

    return 0;
}
