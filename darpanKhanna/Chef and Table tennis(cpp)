#include <iostream>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        string n;
        cin>>n;
        int ones=0,zeroes=0;
        
        for(int i=0;i<n.size();i++)
        {
            if(n[i]=='1')
            ones++;
            
            else{
                zeroes++;
                
            }
        }
        if(ones>=zeroes)
        {
            cout<<"WIN"<<endl;
        }
        else{
            cout<<"LOSE"<<endl;
        }
    }
    
}
