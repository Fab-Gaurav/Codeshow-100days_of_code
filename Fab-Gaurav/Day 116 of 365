#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,key;
    cin>>n;
    int a[10];
    for(int i=0; i<n; i++)
    {
        cin>>a[i];
    }

    int s=INT_MAX;      //INT_MAX = maximum value of int
    int l=INT_MIN;      //INT_MIN =minimum value of int

    for(int i=0; i<n; i++)
    {
        if(a[i]<s)
        {
            s=a[i];
        }
        if(a[i]>l)
        {
            l=a[i];
        }
    }
    cout<<"Large : "<<l<<endl<<"Small : "<<s;
    return 0;
}
